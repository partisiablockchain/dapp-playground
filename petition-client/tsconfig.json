{
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "target": "es5",
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "esnext"
    ],
    "baseUrl": "./"
  }
}
